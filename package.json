{
  "name": "@diegosalazar27/chakraform",
  "version": "0.0.1",
  "description": "Utility to create forms using chakraUI and react-hook-forms using a json object, also supports zod schema for validation.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiegoSalazar27/chakra-form"
  },
  "keywords": [
    "React",
    "ChakraUI",
    "react-hook-form",
    "zod",
    "form",
    "simple",
    "json-to-form"
  ],
  "author": "Diego Salazar",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@hookform/resolvers": "^3.3.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@formkit/auto-animate": "^0.8.1",
    "@types/react": "^18.2.55",
    "framer-motion": "^11.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "zod": "^3.22.4"
  },
  "dependencies": {}
}
